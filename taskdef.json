{
    "executionRoleArn": "arn:aws:iam::398492223295:role/ecsTaskExecutionRole",
    "containerDefinitions": [{
        "logConfiguration": {
            "logDriver": "awslogs",
            "options": {
                "awslogs-group": "demo-app-log-group",
                "awslogs-region": "us-west-2"
            }
        },
        "portMappings": [{
            "containerPort": 80
        }],
        "cpu": 100,
        "memory": 128,
        "image": "<IMAGE>",
        "name": "demo-app"
    }],
    "requiresCompatibilities": [
        "EC2"
    ],
    "family": "demo-app"
}
