{
    "containerDefinitions": [{
        "logConfiguration": {
            "logDriver": "awslogs",
            "options": {
                "awslogs-group": "demo-app-log-group",
                "awslogs-region": "us-west-2"
            }
        },
        "portMappings": [{
            "containerPort": 80
        }],
        "cpu": 100,
        "memory": 128,
        "image": "<IMAGE>",
        "name": "demo-app"
    }],
    "family": "demo-app"
}
